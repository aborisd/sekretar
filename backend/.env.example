# Database
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/ai_calendar
REDIS_URL=redis://localhost:6379

# AI API Keys (from Week 3-4 Smart Router setup)
ANTHROPIC_API_KEY=sk-ant-xxxxx
OPENAI_API_KEY=sk-xxxxx
GOOGLE_API_KEY=xxxxx

# Authentication
JWT_SECRET=change-me-to-random-string-in-production
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=10080  # 1 week

# Feature Flags
ENABLE_AGENT_SYSTEM=false  # Enable in Week 9-16
ENABLE_VECTOR_MEMORY=true
ENABLE_SMART_ROUTING=true

# CORS (comma-separated list)
CORS_ORIGINS=*

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60
